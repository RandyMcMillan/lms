nvm-exec-pnpm-i:
	. ~/.nvm/nvm.sh && nvm exec pnpm i
nvm-exec-pnpm-rb:
	. ~/.nvm/nvm.sh && nvm exec pnpm rb
nvm-exec-pnpm-run:
	. ~/.nvm/nvm.sh && nvm exec pnpm run
nvm-exec-pnpm-run-build:
	. ~/.nvm/nvm.sh && nvm exec pnpm run build
nvm-exec-pnpm-run-watch:
	. ~/.nvm/nvm.sh && nvm exec pnpm run watch
